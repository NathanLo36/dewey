{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters":{
        "simple": {
            "format": "%(asctime)s %(levelname)s: %(message)s",
            "datefmt": "%c"
        }
    },
    "handlers":{
        "file": {
            "class": "logging.handlers.RotatingFileHandler",
            "level": "DEBUG",
            "formatter": "simple",
            "filename": "dewey/logs/log_stream.log",
            "maxBytes": 10000,
            "backupCount": 3
        }
    },
    "loggers": {
        "root": {
            "level": "INFO",
            "handlers":["file"],
            "propagate": false
        }
    }
}